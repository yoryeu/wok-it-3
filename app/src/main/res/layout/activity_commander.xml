<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FEFEFE">

    <Button
        android:id="@+id/menu_enfant"
        android:layout_width="209dp"
        android:layout_height="61dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="101dp"
        android:layout_marginBottom="136dp"
        android:background="#4CAF50"
        android:text="menu enfant"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu_etudiant" />

    <Button
        android:id="@+id/menu_etudiant"
        android:layout_width="209dp"
        android:layout_height="61dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="101dp"
        android:layout_marginBottom="66dp"
        android:background="#4CAF50"
        android:text="menu etudiant"
        app:layout_constraintBottom_toTopOf="@+id/menu_enfant"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu_classique" />

    <Button
        android:id="@+id/menu_classique"
        android:layout_width="209dp"
        android:layout_height="61dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="268dp"
        android:layout_marginEnd="101dp"
        android:layout_marginBottom="52dp"
        android:background="#4CAF50"
        android:backgroundTint="#004CAF50"
        android:text="menu classique"
        app:layout_constraintBottom_toTopOf="@+id/menu_etudiant"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/logowi"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="-14dp" />
</androidx.constraintlayout.widget.ConstraintLayout>